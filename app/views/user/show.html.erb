<div role="main" class="main-content">
  <div class="jumbotron jumbotron-user jumbotron-inverse">
    <div class='container-fluid'>
      <div class='col-lg-12 landing-page'>
        <div class='col-sm-3 col-sm-offset-1'>
          <img src=<%= @user.image %> class='img-thumbnail'>
          <h2 class='text-cente'><%= @user.name %></h2>
          <p class='bottom'><%= @user.screen_name %></p>
        </div>
        <div class='col-sm-3 col-sm-offset-1'>
          <%#<p><%= @user.location %></p>%>
          <%#<p><%= @user.email %></p>%>
          <%#<p><%=%>
        <div class='text-center'>
          <ul class='col-sm-offset-9'>
            <h3>Repos</h3>
          <% @repos.each do |repo| %>
            <li><%= repo.name %></li>
          <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>

